<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.repair.entity">
	<class name="DictJcFixMileage" table="DICT_JC_FIX_MILEAGE">
		<cache usage="read-write" />
	
		<id name="id" column="id" type="java.lang.Long">
			<generator class="sequence">
				<param name="sequence">SEQ_DICT_JC_FIX_MILEAGE</param>
			</generator>
		</id>
		
		<property name="fixMileageValue" column="FIX_MILEAGE_VALUE"></property>
		
		<property name="mileageType" column="MILEAGE_TYPE"></property>
		
		<property name="remark" column="REMARK"></property>
	
	</class>
</hibernate-mapping>